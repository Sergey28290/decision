String patchedValue = vars.get(&quot;patched_value&quot;);
String responseCode = prev.getResponseCode();
String dataLine = postValue + &quot;,&quot; + patchedValue + &quot;,&quot; + responseCode + System.getProperty(&quot;line.separator&quot;);
